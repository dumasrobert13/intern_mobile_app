#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Admin/Desktop/Gerson School Files/Internship Coding Files/intern_mobile_app/backend/node_modules/.pnpm/protobufjs-cli@1.1.1_protobufjs@7.2.4/node_modules/protobufjs-cli/bin/node_modules:/mnt/c/Users/Admin/Desktop/Gerson School Files/Internship Coding Files/intern_mobile_app/backend/node_modules/.pnpm/protobufjs-cli@1.1.1_protobufjs@7.2.4/node_modules/protobufjs-cli/node_modules:/mnt/c/Users/Admin/Desktop/Gerson School Files/Internship Coding Files/intern_mobile_app/backend/node_modules/.pnpm/protobufjs-cli@1.1.1_protobufjs@7.2.4/node_modules:/mnt/c/Users/Admin/Desktop/Gerson School Files/Internship Coding Files/intern_mobile_app/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Admin/Desktop/Gerson School Files/Internship Coding Files/intern_mobile_app/backend/node_modules/.pnpm/protobufjs-cli@1.1.1_protobufjs@7.2.4/node_modules/protobufjs-cli/bin/node_modules:/mnt/c/Users/Admin/Desktop/Gerson School Files/Internship Coding Files/intern_mobile_app/backend/node_modules/.pnpm/protobufjs-cli@1.1.1_protobufjs@7.2.4/node_modules/protobufjs-cli/node_modules:/mnt/c/Users/Admin/Desktop/Gerson School Files/Internship Coding Files/intern_mobile_app/backend/node_modules/.pnpm/protobufjs-cli@1.1.1_protobufjs@7.2.4/node_modules:/mnt/c/Users/Admin/Desktop/Gerson School Files/Internship Coding Files/intern_mobile_app/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/pbjs" "$@"
else
  exec node  "$basedir/../../bin/pbjs" "$@"
fi
